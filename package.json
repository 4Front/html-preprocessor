  },
  "homepage": "https://github.com/4front/htmlprep",
  "dependencies": {
    "debug": "^2.2.0",
    "domelementtype": "^1.3.0",
    "domhandler": "^2.3.0",
    "domutils": "^1.5.1",
    "entities": "^1.1.1",
    "glob": "^7.0.3",
    "lodash": "^4.6.1",
